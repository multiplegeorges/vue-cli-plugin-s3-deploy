{
  "name": "vue-cli-plugin-s3-deploy",
  "version": "4.0.0-rc4",
  "description": "A vue-cli plugin for deploying your built Vue app to an S3 bucket.",
  "scripts": {
    "test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --coverage",
    "watch-test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --watch",
    "build": "trash -f ./*.js && babel ./ ./src/**/*.ts",
    "build-watch": "babel ./src/**/*.ts --watch --out-dir ./",
    "prepublish": "npm build",
    "lint": "yarn eslint . --ext .ts",
    "lint:fix": "yarn eslint . --ext .ts --fix"
  },
  "keywords": [
    "vue",
    "vuejs",
    "cli",
    "plugin",
    "s3",
    "aws",
    "cloudFront",
    "deploy",
    "deployment"
  ],
  "main": "./index.js",
  "homepage": "https://github.com/multiplegeorges/vue-cli-plugin-s3-deploy",
  "author": {
    "name": "Georges Gabereau",
    "email": "s3-deploy@georg.es",
    "url": "https://georg.es"
  },
  "license": "MIT",
  "private": false,
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@hapi/joi": "^16.1.4",
    "@vue/cli-shared-utils": "^4.2.3",
    "aws-sdk": "^2.225.1",
    "es6-promise-pool": "^2.5.0",
    "fs-extra": "9.0.0",
    "globby": "^8.0.1",
    "lodash": "^4.17.11",
    "micromatch": "^4.0.2",
    "mime-types": "^2.1.18",
    "trash-cli": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "25.2.4",
    "typescript": "^4.0.2"
  },
  "files": [
    "bucket.js",
    "configuration.js",
    "connection.js",
    "deployer.js",
    "generator.js",
    "index.js",
    "helper.js",
    "post-tasks.js",
    "prompts.js",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test_assets"
    ]
  }
}
