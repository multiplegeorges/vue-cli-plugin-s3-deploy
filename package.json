{
  "name": "vue-cli-plugin-s3-deploy",
  "version": "4.0.0-rc4",
  "description": "A vue-cli plugin for deploying your built Vue app to an S3 bucket.",
  "scripts": {
    "test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --coverage",
    "watch-test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --watch",
    "build": "rm -f ./*.js && babel ./src/**/*.js --out-dir ./",
    "build-watch": "babel ./src/**/*.js --watch --out-dir ./",
    "prepublish": "npm build",
    "lint": "npx eslint ./src/**/*.js",
    "lint:fix": "npx eslint --fix ./src/**/*.js"
  },
  "keywords": [
    "vue",
    "vuejs",
    "cli",
    "plugin",
    "s3",
    "aws",
    "cloudFront",
    "deploy",
    "deployment"
  ],
  "main": "./index.js",
  "homepage": "https://github.com/multiplegeorges/vue-cli-plugin-s3-deploy",
  "author": {
    "name": "Georges Gabereau",
    "email": "s3-deploy@georg.es",
    "url": "https://georg.es"
  },
  "license": "MIT",
  "private": false,
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^16.1.4",
    "@vue/cli-shared-utils": "^4.2.3",
    "aws-sdk": "^2.225.1",
    "es6-promise-pool": "^2.5.0",
    "fs-extra": "9.0.0",
    "globby": "^8.0.1",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "eslint": "^6.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "25.2.4"
  },
  "files": [
    "bucket.js",
    "configuration.js",
    "connection.js",
    "deployer.js",
    "generator.js",
    "index.js",
    "helper.js",
    "post-tasks.js",
    "prompts.js",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test_assets"
    ]
  }
}
